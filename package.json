{
  "name": "charonium",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev:db:build": "docker compose -f docker-compose.dev.yml --env-file .serve.env up --build dev_db",
    "dev:db:up": "docker compose -f docker-compose.dev.yml --env-file .serve.env up -d dev_db",
    "dev:db:down": "docker-compose -f docker-compose.dev.yml --env-file .serve.env stop dev_db && docker-compose -f docker-compose.dev.yml --env-file .serve.env rm -f dev_db",
    "dev:db:generate": "dotenv -e .serve.env -- npx prisma generate",
    "dev:db:status": "dotenv -e .serve.env -- npx prisma migrate status",
    "dev:db:migrate": "dotenv -e .serve.env -- npx prisma migrate dev",
    "dev:db:deploy": "dotenv -e .serve.env -- npx prisma migrate deploy",
    "dev:db:studio": "dotenv -e .serve.env -- npx prisma studio",
    "e2e:db:build": "docker-compose -f docker-compose.e2e.yml --env-file .e2e.env build test_db",
    "e2e:db:up": "docker-compose -f docker-compose.e2e.yml --env-file .e2e.env up -d --remove-orphans test_db",
    "e2e:db:down": "docker-compose -f docker-compose.e2e.yml --env-file .e2e.env stop test_db && docker-compose -f docker-compose.e2e.yml --env-file .e2e.env rm -f test_db",
    "e2e:db:deploy": "dotenv -e .e2e.env -- npx prisma migrate deploy",
    "e2e:start": "nx e2e customer-api > coverage/customer-api-e2e/output.log 2>&1 & echo $! > coverage/customer-api-e2e/pid.log",
    "e2e:stop": "kill $(cat coverage/customer-api-e2e/pid.log) && rm coverage/customer-api-e2e/pid.log",
    "e2e:ngrok:start": "dotenv -e .e2e.env -- bash -c 'nohup ngrok http $PORT > /dev/null 2>&1 & echo $! > coverage/customer-api-e2e/ngrok_pid.log'",
    "e2e:ngrok:stop": "kill $(cat coverage/customer-api-e2e/ngrok_pid.log) && rm coverage/customer-api-e2e/ngrok_pid.log",
    "test:unit": "TEST_TYPE=unit nx run customer-api:test",
    "test:e2e": "TEST_TYPE=e2e nx run customer-api-e2e:e2e"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/eslint-plugin-nx": "15.8.7",
    "@nrwl/jest": "15.8.7",
    "@nrwl/js": "15.8.7",

    "@nrwl/nest": "^15.8.7",
    "@nrwl/node": "15.8.7",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/webpack": "15.8.7",
    "@nrwl/workspace": "15.8.7",
    "@types/coinbase-commerce-node": "^1.0.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
